# Database
DATABASE_URL=postgresql://user:password@localhost:5432/prestige_auto

# Test Database (used by integration tests)
DATABASE_URL_TEST=postgresql://user:password@localhost:5432/prestige_auto_test

# GoHighLevel CRM Integration
GHL_API_TOKEN=your_ghl_api_token
GHL_LOCATION_ID=your_ghl_location_id

# Push Notifications (VAPID keys)
VAPID_PUBLIC_KEY=your_vapid_public_key
